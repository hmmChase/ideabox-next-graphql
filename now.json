{
  "name": "ideabox-next-graphql",
  "version": 2,
  "builds": [
    { "src": "frontend/package.json", "use": "@now/next" },
    { "src": "backend/src/index.js", "use": "@now/node-server" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "frontend/$1" },
    { "src": "/api/(.*)", "dest": "backend/src/index.js" }
  ]
}
